{
"manifest_version": 3,
  "name": "PhishGUard MK2",
  "description": "Version MK2 1.0 ",
  "version": "1.1",
  "icons": {
    "16": "images/Guard_MK1.png",
    "32": "images/Guard_MK1.png",
    "48": "images/Guard_MK1.png",
    "64": "images/Guard_MK1.png",
    "128": "images/Guard_MK1.png",
    "256": "images/Guard_MK1.png"
  },
  "permissions": ["tabs", "storage", "scripting", "activeTab"],
  "host_permissions": ["https://script.google.com/macros/*","https://mail.google.com/*"],

  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["test.js","Highlight_Words.js"]
    }
  ],
  "action": 
  {
    "default_popup": "Extention.html",
    "default_title": "PhishGuard Mk2",
    "default_icon": {
      "16": "images/Guard_MK1.png",
      "32": "images/Guard_MK1.png",
      "48": "images/Guard_MK1.png",
      "64": "images/Guard_MK1.png",
      "128": "images/Guard_MK1.png",
      "256": "images/Guard_MK1.png"
    }
  }
}